{
  "name": "ReactNativeStarterKit",
  "description": "It is the simple starter kit for mobile applications based on ES6, React Native and Flux technologies.",
  "version": "1.4.0",
  "author": {
    "name": "Marat Fakhreev",
    "email": "marat.fakhreev@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/maratfakhreev/react-native-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/maratfakhreev/react-native-starter-kit/issues"
  },
  "license": "MIT",
  "engines": {
    "node": "6.6.x"
  },
  "scripts": {
    "start": "./node_modules/react-native/packager/packager.sh start",
    "reset": "watchman watch-del-all && ./node_modules/react-native/packager/packager.sh start --reset-cache",
    "upgrade": "npm install -g react-native-cli && react-native upgrade",
    "lint": "eslint app",
    "test": "npm run lint"
  },
  "dependencies": {
    "alt": "~0.18.6",
    "alt-utils": "~1.0.0",
    "deep-assign": "~2.0.0",
    "lodash": "~4.16.0",
    "react": "~15.3.2",
    "react-native": "~0.33.0"
  },
  "devDependencies": {
    "babel-cli": "~6.14.0",
    "babel-eslint": "~6.1.2",
    "babel-preset-react-native-stage-0": "~1.0.1",
    "eslint": "~3.5.0",
    "eslint-plugin-import": "~1.15.0",
    "eslint-plugin-react": "~6.2.2"
  }
}
